    "clean": "rm -rf prod && mkdir prod",
    "build:frontend": "webpack -p",
    "build:backend": "babel . --ignore node_modules,src,webpack.config.js --out-dir prod",
    "copy": "cp package.json prod/ && mkdir prod/src && cp src/index.html prod/src/index.html && cp -a src/public prod/src/public",
